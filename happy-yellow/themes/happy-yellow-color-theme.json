{
  "name": "happy-yellow",
  "type": "light",
  "colors": {
    "editor.background": "#FFFFE0",
    "editor.foreground": "#000000",
    "editorCursor.foreground": "#000000",
    "editor.lineHighlightBackground": "#FFFFE0",
    "editorLineNumber.foreground": "#000000",
    "titleBar.activeBackground": "#FFFF00",
    "titleBar.activeForeground": "#000000",
    "titleBar.inactiveBackground": "#FFFFE0",
    "titleBar.inactiveForeground": "#A0A0A0",
    "activityBar.background": "#FFFF00",
    "activityBar.foreground": "#000000",
    "statusBar.background": "#FFFF00",
    "statusBar.foreground": "#000000",
    "button.background": "#FFFF00",
    "button.foreground": "#000000",
    "input.background": "#FFFFFF",
    "input.foreground": "#000000",
    "input.placeholderForeground": "#A0A0A0"
  },
  "tokenColors": [
    {
      "scope": ["comment", "comment.block"],
      "settings": {
        "foreground": "#A0A0A0"
      }
    },
    {
      "scope": ["keyword", "variable.language"],
      "settings": {
        "foreground": "#0000FF"
      }
    },
    {
      "scope": ["string", "constant"],
      "settings": {
        "foreground": "#008000"
      }
    },
    {
      "scope": ["variable", "variable.parameter"],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": ["function", "entity.name.function"],
      "settings": {
        "foreground": "#A52A2A"
      }
    }
  ],
  "semanticHighlighting": true
}